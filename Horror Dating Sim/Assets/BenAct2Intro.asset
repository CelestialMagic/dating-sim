        _value: {x: 0, y: 0}
      StartingDimensions:
        _isEnabled: 0
        _value: {x: 0, y: 0}
      ImageSearchParameters:
        _isEnabled: 1
        _value:
          _imageType: 1
          _version: 1
    _line: (Whoa... He's concerned about me? That's so sweet...)
  - _speakerProperties:
    - _speakerIndex: -1
      _isHidden: 0
    _characterSpriteSettings:
    - IsSubject:
        _isEnabled: 1
